{
  "name": "study-hour-calendar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "nodemon",
    "build": "next build",
    "build:server": "tsc --project server/tsconfig.json",
    "build:all": "npm run build:server && npm run build",
    "start": "next start",
    "start:backend": "node server/dist/index.js",
    "lint": "next lint",
    "test": "vitest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "next": "14.2.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.33",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^1.5.0"
  }
}
